{
  "name": "chargease-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "ChargEase - Nepal's Premier EV Charging Network Frontend",
  "author": "ChargEase Nepal",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host --open",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview --host",
    "build:analyze": "vite build --mode analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit",
    "seo:check": "echo 'SEO optimization check - see SEO_OPTIMIZATION_GUIDE.md for details'",
    "seo:build": "npm run build && echo 'Production build with SEO optimizations complete'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^5.1.1",
    "@tanstack/react-query": "^5.80.7",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "firebase": "^11.9.1",
    "framer-motion": "^12.18.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.515.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.58.0",
    "react-hot-toast": "^2.5.2",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5"
  },
  "overrides": {
    "picomatch": "4.0.2"
  },
  "keywords": [
    "react",
    "vite",
    "tailwindcss",
    "ev-charging",
    "nepal",
    "electric-vehicle",
    "charging-station",
    "booking-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chargease-nepal/frontend.git"
  },
  "homepage": "https://chargease.com.np",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
